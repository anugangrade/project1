
<div class="col-lg-12 wh_col">
  <h3 class="cr_gig">Sign up</h3> 
  <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
    <%= devise_error_messages! %>


    <div class="full_gig_blog">
      <div class="labels">EMAIL</div>
      <div class="fields">
        <%= f.email_field :email, autofocus: true, :placeholder=>"Email" , :class=>"form-control" %>
      </div>
    </div><!--full_blog-->

    <div class="full_gig_blog">
      <div class="labels">NAME</div>
      <div class="fields">
        <%= f.text_field :name, :placeholder=>"Name", :class=>"form-control" %>
      </div>
    </div><!--full_blog-->

    <div class="full_gig_blog">
      <div class="labels">PASSWORD</div>
      <div class="fields">
        <%= f.password_field :password, :placeholder=>"********", :class=>"form-control" %>
      </div>
    </div><!--full_blog-->

    <div class="full_gig_blog">
      <div class="labels">CONFIRM YOUR <br/>PASSWORD</div>
      <div class="fields">
        <%= f.password_field :password_confirmation, :placeholder=>"********", :class=>"form-control" %>
      </div>
    </div><!--full_blog-->


    <div class="full_gig_blog">
      <div class="fields">
        <%= f.submit "Sign up", :class=>"btn btn-success" %> OR <%= render "devise/shared/links" %>
      </div>
    </div><!--full_blog-->
  <% end %>

</div><!-- 9 col-->   

